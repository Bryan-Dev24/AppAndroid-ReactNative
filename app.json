{
  "name": "AppAndroidReactNative",
  "displayName": "App Android React Native",
  "expo": {
    "name": "App Android React Native",
    "slug": "App Android built with React-Native & Expo Default template",
    "scheme": "appandroid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets//images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "#"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bryanjames.appandroidreactnative",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.bryanjames.appandroidreactnative",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "appandroid_reactnative_expo"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
