# Configurações da API
API_URL=your_api_url_here
API_KEY=your_api_key_here

# Configurações do Aplicativo
APP_NAME=AppAndroidReactNative
APP_ENV=development
APP_DEBUG=true

# Configurações de Autenticação
AUTH_TOKEN_KEY=auth_token
REFRESH_TOKEN_KEY=refresh_token
TOKEN_EXPIRATION=24h

# Configurações de Log
LOG_LEVEL=debug

# Configurações de Tempo
API_TIMEOUT=30000

# Configurações de Cache
CACHE_ENABLED=true
CACHE_TTL=3600

# Configurações de Internacionalização
DEFAULT_LANGUAGE=pt-BR
SUPPORTED_LANGUAGES=pt-BR,en-US,es-ES

# Configurações de Tema
DEFAULT_THEME=light

# Configurações de URL
DEEP_LINKING_PREFIX=com.bryanjames.appandroidreactnative://

# Configurações do Sentry (opcional)
SENTRY_DSN=your_sentry_dsn_here
ENABLE_SENTRY=false

# Configurações do OneSignal (opcional)
ONE_SIGNAL_APP_ID=your_onesignal_app_id
ONE_SIGNAL_REST_API_KEY=your_onesignal_rest_api_key

# Configurações do Google Maps (opcional)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Configurações de Pagamento (opcional)
STRIPE_PUBLIC_KEY=your_stripe_public_key
PAYPAL_CLIENT_ID=your_paypal_client_id
